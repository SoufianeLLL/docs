{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Vlauma Analytics",
  "colors": {
    "primary": "#A855F7",
    "light": "#A855F7",
    "dark": "#A855F7"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "features/markdown",
              "features/code",
              "features/images",
              "features/settings",
              "features/navigation",
              "features/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "Changelog",
        "groups": [
          {
            "group": "Product Updates",
            "pages": [
              "changelog/introduction"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Let's Talk",
          "href": "https://vlauma.com/support/contact",
          "icon": "support"
        },
        {
          "anchor": "FAQs",
          "href": "https://vlauma.com/faqs",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@vlauma.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://vlauma.com/dashboard"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/vlauma",
      "facebook": "https://github.com/vlaumacom",
      "linkedin": "https://linkedin.com/company/vlauma"
    }
  }
}